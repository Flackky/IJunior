using System;

class Program
{
    static void Main(string[] args)
    {
        int appointmentTime = 10;
        int minutesInHour = 60;

        Console.WriteLine("Введите количество старушек: "); int numberOfPatients = int.Parse(Console.ReadLine());
        int hoursInQueue = appointmentTime * numberOfPatients / minutesInHour;
        int minutesInQueue = appointmentTime * numberOfPatients % minutesInHour;
        Console.WriteLine($"Вы должны отстоять в очереди {hoursInQueue} часов и {minutesInQueue} минут.");
    }
}
